<html>
<head>
    <title>Add New Resume</title>
</head>
<body>
<h1>Add New Resume</h1>
<form action="/resume/insert" method="get">
    Resume Name: <input id="resume_name" name="resume_name" type="text" />
    <br />
    <br />
    Account
    <select id="account_id" name="account_id">
        <% for(var i=0; resume.length > i; i++ ) { %>
        <option value="<%= resume[i].account_id %>"><%= resume[i].first_name +" " + resume[i].last_name %></option>
        <% } %>
    </select>

    <input type="submit" />

</form>
</body>

</html>